    <!-- End Header -->

    <main id="main">
      <div class="section-title">
        <h2>View Accounts</h2>
      </div>
      <!-- Table -->
      <!-- Add the tab structure -->
      <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item" role="presentation">
          <a
            class="nav-link active"
            id="student-tab"
            data-bs-toggle="tab"
            href="#student"
            role="tab"
            aria-controls="student"
            aria-selected="true"
            >Students</a
          >
        </li>
        <li class="nav-item" role="presentation">
          <a
            class="nav-link"
            id="counselor-tab"
            data-bs-toggle="tab"
            href="#counselor"
            role="tab"
            aria-controls="counselor"
            aria-selected="false"
            >Counselors</a
          >
        </li>
        <li class="nav-item" role="presentation">
          <a
            class="nav-link"
            id="studentArchive-tab"
            data-bs-toggle="tab"
            href="#studentArchive"
            role="tab"
            aria-controls="studentArchive"
            aria-selected="false"
            >Archived Students</a
          >
        </li>
        <li class="nav-item" role="presentation">
          <a
            class="nav-link"
            id="counselorArchive-tab"
            data-bs-toggle="tab"
            href="#counselorArchive"
            role="tab"
            aria-controls="counselorArchive"
            aria-selected="false"
            >Archived Counselors</a
          >
        </li>
      </ul>

      <div class="tab-content" id="myTabContent">
        <!-- Students Tab Content -->
        <div
          class="tab-pane fade show active"
          id="student"
          role="tabpanel"
          aria-labelledby="student-tab"
        >
          <div class="table-responsive">
            <table class="table border border-2 table-hover">
              <thead class="table-dark">
                <tr>
                  <th scope="col">Email</th>
                  <th scope="col">Name</th>
                  <th scope="col">Program Code</th>
                  <th scope="col">ID</th>
                  <th scope="col">Birthdate</th>
                  <th scope="col">Gender</th>
                  <th scope="col">Department</th>
                  <th scope="col">Phone Number</th>
                  <th scope="col">Actions</th>
                </tr>
              </thead>
              <tbody>
                <% studentViewAccounts.forEach((item) => { %>
                <tr>
                  <th scope="row"><%= item.email %></th>
                  <td><%= item.last_name %> <%= item.first_name %></td>
                  <td><%= item.progCode %></td>
                  <td><%= item.id_number %></td>
                  <td><%= item.birth_date %></td>
                  <td><%= item.gender %></td>
                  <td><%= item.department %></td>
                  <td><%= item.phone_number %></td>
                  <td>
                    <button
                      class="btn btn-outline-success edit-student"
                      data-toggle="modal"
                      data-target="#editStudentModal"
                      studentEmail="<%= item.email %>"
                    >
                      Edit
                    </button>
                    <button
                      class="btn btn-outline-danger delete-student"
                      data-toggle="modal"
                      data-target="#deleteStudentModal"
                      studentEmail="<%= item.email %>"
                    >
                      Archive
                    </button>
                  </td>
                </tr>
                <% }); %>
              </tbody>
            </table>
          </div>
        </div>
        <!-- Counselors Tab Content -->
        <div
          class="tab-pane fade"
          id="counselor"
          role="tabpanel"
          aria-labelledby="counselor-tab"
        >
          <div class="table-responsive">
            <table class="table border border-2 table-hover">
              <thead class="table-dark">
                <tr>
                  <th scope="col">Email</th>
                  <th scope="col">Name</th>
                  <th scope="col">ID</th>
                  <th scope="col">Birthdate</th>
                  <th scope="col">Gender</th>
                  <th scope="col">Phone Number</th>
                  <th scope="col">Actions</th>
                </tr>
              </thead>
              <tbody>
                <% counselorViewAccounts.forEach((item2) => { %>
                <tr>
                  <th scope="row"><%= item2.email %></th>
                  <td><%= item2.last_name %> <%= item2.first_name %></td>
                  <td><%= item2.id_number %></td>
                  <td><%= item2.birth_date %></td>
                  <td><%= item2.gender %></td>
                  <td><%= item2.phone_number %></td>
                  <td>
                    <button
                      class="btn btn-outline-success edit-counselor"
                      data-toggle="modal"
                      data-target="#editCounselorModal"
                      counselorEmail="<%= item2.email %>"
                    >
                      Edit
                    </button>
                    <button
                      class="btn btn-outline-danger delete-counselor"
                      data-toggle="modal"
                      data-target="#deleteCounselorModal"
                      counselorEmail="<%= item2.email %>"
                    >
                      Archive
                    </button>
                  </td>
                </tr>
                <% }); %>
              </tbody>
            </table>
          </div>
        </div>
        <!-- Archived Students Tab Content -->
        <div class="tab-pane fade" id="studentArchive" role="tabpanel" aria-labelledby="student-tab">
          <div class="table-responsive">
            <% if (studentArchiveViewAccounts.length === 0) { %>
              <h2 style="margin-top: 30px;" class="text-center">No data found</h2>
            <% } else { %>
              <table class="table border border-2 table-hover">
                <thead class="table-dark">
                  <tr>
                    <th scope="col">Email</th>
                    <th scope="col">Name</th>
                    <th scope="col">ID</th>
                    <th scope="col">Birthdate</th>
                    <th scope="col">Gender</th>
                    <th scope="col">Phone Number</th>
                    <th scope="col">Actions</th>
                  </tr>
                </thead>
                <tbody>
                  <% studentArchiveViewAccounts.forEach((item2) => { %>
                    <tr>
                      <th scope="row"><%= item2.email %></th>
                      <td><%= item2.last_name %> <%= item2.first_name %></td>
                      <td><%= item2.id_number %></td>
                      <td><%= item2.birth_date %></td>
                      <td><%= item2.gender %></td>
                      <td><%= item2.phone_number %></td>
                      <td>
                        <button class="btn btn-outline-danger retrieve-student"
                                data-toggle="modal"
                                data-target="#retrieveStudentModal"
                                studentEmail="<%= item2.email %>">
                          Retrieve
                        </button>
                      </td>
                    </tr>
                  <% }); %>
                </tbody>
              </table>
            <% } %>
          </div>
        </div>
        
        <!-- Archived Counselors Tab Content -->
        <div
          class="tab-pane fade"
          id="counselorArchive"
          role="tabpanel"
          aria-labelledby="counselor-tab"
        >
          <div class="table-responsive">
            <% if (counselorArchiveViewAccounts.length === 0) { %>
              <h2 style="margin-top: 30px;" class="text-center">No data found</h2>
            <% } else { %>
            <table class="table border border-2 table-hover">
              <thead class="table-dark">
                <tr>
                  <th scope="col">Email</th>
                  <th scope="col">Name</th>
                  <th scope="col">ID</th>
                  <th scope="col">Birthdate</th>
                  <th scope="col">Gender</th>
                  <th scope="col">Phone Number</th>
                  <th scope="col">Actions</th>
                </tr>
              </thead>
              <tbody>
                <% counselorArchiveViewAccounts.forEach((item2) => { %>
                <tr>
                  <th scope="row"><%= item2.email %></th>
                  <td><%= item2.last_name %> <%= item2.first_name %></td>
                  <td><%= item2.id_number %></td>
                  <td><%= item2.birth_date %></td>
                  <td><%= item2.gender %></td>
                  <td><%= item2.phone_number %></td>
                  <td>
                    <button
                      class="btn btn-outline-danger retrieve-counselor"
                      data-toggle="modal"
                      data-target="#retrieveCounselorModal"
                      counselorEmail="<%= item2.email %>"
                    >
                      Retrieve
                    </button>
                  </td>
                </tr>
                <% }); %>
              </tbody>
            </table>
            <% } %>
          </div>
        </div>
      </div>
    </main>
    <!-- End #main -->

