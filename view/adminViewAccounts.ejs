    <style>
      body {
        display: flex;
        flex-direction: column;
        min-height: 100vh;
      }

      main {
        flex: 1; /* This makes the main content take up the remaining vertical space */
      }

      /* If needed, adjust the footer styles below */
      #footer {
        padding: 30px 0;
        text-align: center;
        width: 100%;
      }

      .table-responsive {
        max-width: 80%; /* Adjust the maximum width as needed */
        margin: 0 auto; /* Center the table */
      }

      .table {
        border: 1px solid #dee2e6;
        border-radius: 0.25rem;
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
      }

      th,
      td {
        padding: 1rem;
        text-align: left;
        border-bottom: 1px solid #dee2e6;
      }

      th {
        background-color: #007bff;
        color: #fff;
      }

      tbody tr:hover {
        background-color: #f8f9fa;
      }

      .dataTables_wrapper .dataTables_paginate .paginate_button {
        margin-right: 5px; /* Adjust the spacing as needed */
      }
      /* a */
      table {
        margin: 0 auto;
      }

      section {
        padding: 60px 0;
      }

      .section-bg {
        background-color: #f8fcf9;
      }

      .section-title {
        text-align: center;
        padding-bottom: 30px;
      }

      .section-title h2 {
        font-size: 32px;
        font-weight: bold;
        text-transform: uppercase;
        margin-bottom: 20px;
        padding-bottom: 20px;
        position: relative;
      }

      .section-title h2::before {
        content: "";
        position: absolute;
        display: block;
        width: 120px;
        height: 1px;
        background: #ddd;
        bottom: 1px;
        left: calc(50% - 60px);
      }

      .section-title h2::after {
        content: "";
        position: absolute;
        display: block;
        width: 40px;
        height: 3px;
        background: #5cb874;
        bottom: 0;
        left: calc(50% - 20px);
      }

      .section-title p {
        margin-bottom: 0;
        font-size: 14px;
        color: #919191;
      }
    </style>
    <!--Delete Student Modal -->
    <div
      class="modal fade"
      id="deleteStudentModal"
      tabindex="-1"
      role="dialog"
      aria-labelledby="deleteStudentModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="deleteStudentModalLabel">
              Archive Account
            </h5>

            <button
              type="button"
              class="close close-modal-btn"
              data-dismiss="modal"
              id="closeDeleteModalX1"
              aria-label="Close"
            >
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <p>Are you sure you want to archive this Student account?</p>
          </div>
          <div class="modal-footer">
            <button
              type="button"
              class="btn btn-danger confirm-deleteStudent"
              data-dismiss="modal"
            >
              Confirm Archive
            </button>
            <button
              type="button"
              class="close close-modal-btn"
              data-dismiss="modal"
              aria-label="Close"
              id="closeDeleteBtn1"
            >
              Close
            </button>
          </div>
        </div>
      </div>
    </div>
    <!--Delete Counselor Modal -->
    <div
      class="modal fade"
      id="deleteCounselorModal"
      tabindex="-1"
      role="dialog"
      aria-labelledby="deleteCounselorModalModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="deleteCounselorModalLabel">
              Archive Account
            </h5>

            <button
              type="button"
              class="close close-modal-btn"
              data-dismiss="modal"
              id="closeDeleteModalX2"
              aria-label="Close"
            >
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <p>Are you sure you want to archive this Counselor account?</p>
          </div>
          <div class="modal-footer">
            <button
              type="button"
              class="btn btn-danger confirm-deleteCounselor"
              data-dismiss="modal"
            >
              Confirm Archive
            </button>
            <button
              type="button"
              class="close close-modal-btn"
              data-dismiss="modal"
              aria-label="Close"
              id="closeDeleteBtn2"
            >
              Close
            </button>
          </div>
        </div>
      </div>
    </div>

    <!-- Edit Student Modal-->
    <div
      class="modal fade"
      id="editStudentModal"
      tabindex="-1"
      role="dialog"
      aria-labelledby="editStudentModal"
      aria-hidden="true"
    >
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="editStudentModalLabel">
              Edit Student Account
            </h5>
            <button
              type="button"
              class="close"
              data-dismiss="modal"
              id="closeEditModalX1"
              aria-label="Close"
            >
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <!-- Add your form elements here -->
            <form id="counselingForm">
              <div class="mb-3">
                <label for="firstName" class="form-label">First Name:</label>
                <input type="input" class="form-control" id="firstName" />
              </div>
              <div class="mb-3">
                <label for="lastName" class="form-label">Last Name:</label>
                <input type="input" class="form-control" id="lastName" />
              </div>
              <div class="mb-3">
                <label for="idNumber" class="form-label">ID Number:</label>
                <input type="input" class="form-control" id="idNumber" />
              </div>
              <div class="mb-3">
                <label for="gender" class="form-label">Gender:</label>
                <div class="form-check">
                  <input
                    class="form-check-input"
                    type="radio"
                    name="gender"
                    id="MALE"
                    value="MALE"
                  />
                  <label class="form-check-label" for="MALE">Male</label>
                </div>
                <div class="form-check">
                  <input
                    class="form-check-input"
                    type="radio"
                    name="gender"
                    id="FEMALE"
                    value="FEMALE"
                  />
                  <label class="form-check-label" for="FEMALE">Female</label>
                </div>
              </div>
              <div class="mb-3">
                <label for="department" class="form-label">Department:</label>
                <select class="form-select" id="department">
                  <option value="" disabled>Select Department</option>
                  <option value="CSCS">CSCS</option>
                  <option value="CEAT">CEAT</option>
                  <option value="CTHM">CTHM</option>
                  <option value="CLAC">CLAC</option>
                  <option value="COED">COED</option>
                  <option value="CCJE">CCJE</option>
                  <option value="CBAA">CBAA</option>
                </select>
              </div>
              <div class="mb-3">
                <label for="programCode" class="form-label"
                  >Program Code:</label
                >
                <input type="input" class="form-control" id="programCode" />
              </div>
            </form>
          </div>
          <div class="modal-footer">
            <div class="modal-footer">
              <button
                type="button"
                class="btn btn-primary confirm-editStudent"
                data-dismiss="modal"
              >
                Confirm Edit
              </button>
              <button
                type="button"
                class="close close-modal-btn"
                data-dismiss="modal"
                aria-label="Close"
                id="closeEditBtn1"
              >
                Close
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>

